create table student (id number, first_name varchar2(32), last_name varchar2(32), email varchar2(64));

create sequence STUDENT_ID_SEQ  MINVALUE 1
  MAXVALUE 9999
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
  
  
